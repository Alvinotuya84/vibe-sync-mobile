{
  "expo": {
    "name": "vibe-sync",
    "slug": "vibe-sync",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vibe-sync",
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vibesync",
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "react-native-video",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "com.merchant.vibesync.",
          "enableGooglePay": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5f299cdc-71bd-4686-8b22-1112fefe7169"
      }
    },
    "owner": "alvindo",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/5f299cdc-71bd-4686-8b22-1112fefe7169",
      "requestHeaders": {
        "expo-channel-name": "production-1.0.0"
      }
    }
  }
}
